<div class="community-benefits-container">

    <div class="divider">
        <div class="divider-rect">
        </div>
    </div>

    <div class="title">
        <h1>{{contentMainObject.title}}</h1>
    </div>

    <div class="benefits-content-container">
        <div class="benefit-content-item">
            <img src="{{contentObjectLeftUp.image}}" class="benefit-content-image">
                
            <div class="title">
                <h1>{{contentObjectLeftUp.title}}</h1>
            </div>
            <div class="description">
                <h1>{{contentObjectLeftUp.description}}</h1>
            </div>

        </div>

        <div class="benefit-content-item">
            <img src="{{contentObjectRightUp.image}}" class="benefit-content-image">
                
            <div class="title">
                <h1>{{contentObjectRightUp.title}}</h1>
            </div>
            <div class="description">
                <h1>{{contentObjectRightUp.description}}</h1>
            </div>

        </div>

        <div class="benefit-content-item">
            <img src="{{contentObjectLeftDown.image}}" class="benefit-content-image">
                
            <div class="title">
                <h1>{{contentObjectLeftDown.title}}</h1>
            </div>
            <div class="description">
                <h1>{{contentObjectLeftDown.description}}</h1>
            </div>

        </div>

        <div class="benefit-content-item">
            <img src="{{contentObjectRightDown.image}}" class="benefit-content-image">

            <div class="title">
                <h1>{{contentObjectRightDown.title}}</h1>
            </div>
            <div class="description">
                <h1>{{contentObjectRightDown.description}}</h1>
            </div>

        </div>

    </div>

    <div class="actions-container">
           <button class="action-button-first">
               {{#link-to "sign-up-membership" target="_blank"}}{{contentMainObject.firstButton}}{{/link-to}}
            </button>
                <button class="action-button-second">
                {{#link-to "work-space"}}{{contentMainObject.secondButton}}{{/link-to}}
            </button>
    </div>

</div>
